{
	"name": "discover-twitter-spaces",
	"version": "1.0.0",
	"license": "MIT",
	"description": "Discover Twitter Spaces. Discover the spaces that are live or scheduled on Twitter.",
	"repository": {
		"type": "git",
		"url": "https://github.com/navneetsharmaui/discover-twitter-spaces.git"
	},
	"author": {
		"name": "Navneet Sharma",
		"url": "https://navneetsharma.xyz"
	},
	"homepage": "https://discover-twitter-spaces.vercel.app/",
	"keywords": [
		"twitter",
		"twitter-spaces",
		"discover",
		"discover-twitter-spaces",
		"discover-twitter-spaces-js",
		"discover-twitter-spaces-ts",
		"discover-twitter-spaces-node",
		"twitter spaces",
		"spaces",
		"live-twitter-spaces",
		"scheduled-twitter-spaces",
		"twitter-spaces-discovery",
		"typescript",
		"javascript",
		"sveltekit",
		"svelte",
		"sveltejs",
		"serverless"
	],
	"scripts": {
		"prepare": "husky install",
		"prebuild": "rimraf .svelte-kit && rimraf build && rimraf .vercel_build_output && yarn run sitemap",
		"pre-commit": "lint-staged",
		"check-env": "node --loader ts-node/esm scripts/js/check-env.ts",
		"sitemap": "node --loader ts-node/esm scripts/js/sitemap.ts --mode=prod --project=discover --arhitect-type=build --configurations=prod",
		"dev:sveltekit": "svelte-kit dev",
		"dev:sveltekit:host": "svelte-kit dev --host=0.0.0.0",
		"dev": "yarn run check-env && yarn run dev:sveltekit",
		"dev:host": "yarn run check-env && yarn run dev:sveltekit:host",
		"dev:open": "yarn run dev --open",
		"dev:host:open": "yarn run dev --open --host",
		"build:env:prod": "node --loader ts-node/esm scripts/js/environment.setup.ts --mode=prod --project=discover --arhitect-type=build --configurations=prod",
		"build:env:dev": "node --loader ts-node/esm scripts/js/environment.setup.ts --mode=dev --project=discover --arhitect-type=build --configurations=dev",
		"build:env:qa": "node --loader ts-node/esm scripts/js/environment.setup.ts --mode=qa --project=discover --arhitect-type=build --configurations=qa",
		"build:env:local": "node --loader ts-node/esm scripts/js/environment.setup.ts --mode=local --project=discover --arhitect-type=build --configurations=local",
		"build:sveltekit": "cross-env-shell svelte-kit build",
		"build": "run-s build:env:prod build:sveltekit build:env:local",
		"build:verbose": "cross-env-shell svelte-kit build --verbose",
		"build:vercel": "cross-env ADAPTER=vercel OPTIONS='{}' yarn run build",
		"build:vercel:verbose": "cross-env ADAPTER=vercel OPTIONS='{}' yarn run build:verbose",
		"preview": "svelte-kit preview",
		"preview:open": "yarn run run preview --open",
		"preview:host": "yarn run run preview --host",
		"preview:host:open": "yarn run run preview --open --host",
		"lint": "prettier --ignore-path .gitignore --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} && eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore",
		"lint:fix": "prettier --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} && eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore",
		"format": "prettier --ignore-path .gitignore --write ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js}",
		"eslint": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore",
		"eslint:fix": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore --fix",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/typography": "^0.5.0",
		"@types/ioredis": "^4.28.5",
		"@types/isomorphic-fetch": "^0.0.35",
		"@types/lz-string": "^1.3.34",
		"@types/node": "^17.0.4",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"autoprefixer": "^10.4.0",
		"cross-env": "^7.0.3",
		"cssnano": "^5.0.14",
		"cssnano-preset-advanced": "^5.1.9",
		"dotenv": "^10.0.0",
		"eslint": "^8.4.1",
		"eslint-config-airbnb-typescript": "^16.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"husky": "^7.0.4",
		"ioredis": "^4.28.2",
		"isomorphic-fetch": "^3.0.0",
		"lint-staged": "^12.1.3",
		"lz-string": "^1.4.4",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.5",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.3",
		"svelte-check": "^2.2.11",
		"svelte-loader": "^3.1.2",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.6",
		"ts-node": "^10.4.0",
		"tslib": "^2.3.1",
		"tsyringe": "^4.6.0",
		"typescript": "^4.5.4"
	},
	"type": "module",
	"engines": {
		"node": ">= 14.16.1"
	},
	"engineStrict": true
}
